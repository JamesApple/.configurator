  "coc.source.include.trimSameExts": [".ts", ".js"],

  "coc.source.omni.enable": false, // Omni is pretty slow allegedly
  "coc.source.omni.shortcut": "O",
  "coc.source.omni.priority": 3,
  "coc.source.omni.filetypes": [],

  "coc.source.tag.enable": true,
