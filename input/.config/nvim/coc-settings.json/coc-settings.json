{
  "coc.preferences.autoTrigger": "trigger",
  "coc.preferences.maxCompleteItemCount": 15,
  "coc.preferences.triggerAfterInsertEnter": false,
  "coc.preferences.customSources": ["word", "emoji", "include"],
  "coc.preferences.timeout": 1000,
  "coc.preferences.noselect": true,
  "coc.preferences.jumpCommand": "edit",

  "coc.preferences.diagnostic.enable": false, // Disable all linting services!! It breaks signature display and just sucks :[

  "coc.source.around.enable": true,
  "coc.source.around.shortcut": "A",
  "coc.source.around.priority": 1,

  "coc.source.buffer.enable": true,
  "coc.source.buffer.shortcut": "B",
