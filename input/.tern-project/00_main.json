{
  "ecmaVersion": 7,
  "libs": ["browser", "ecmascript", "react"],
  "plugins": {
    "doc_comment": {
      "fullDocs": true,
      "strong": true
    },
    "complete_strings": {
      "maxLength": 15
    },
    "es_modules": {},
    "node": {},
    "node_resolve": {},
    "commonjs": {},
    "webpack": {
      "configPath": "./webpack.config.js",
      "modules": ["node_modules"]
    }
  },
  "loadEagerly": [
    "*/*/*/*/*/*/*/*.js",
    "*/*/*/*/*/*/*.js",
    "*/*/*/*/*/*.js",
    "*/*/*/*/*.js",
    "*/*/*/*.js",
    "*/*/*.js",
    "*/*.js",
    "*.js"
  ],
  "dontLoad": ["node_modules/**"]
}
