{
  "ecmaVersion": 6,
  "libs": [
    "browser",
    "vue",
    "react",
    "ecma5",
    "ecma6"
  ],
  "plugins": {
      "doc_comment": {
        "fullDocs": true,
        "strong": true
      },
      "es_modules": {},
      "node": {}
  },
  "loadEagerly": [
    "app/webpacker/**/*.js"
  ],
  "dontLoad": [
      "node_modules/**"
  ]
}
